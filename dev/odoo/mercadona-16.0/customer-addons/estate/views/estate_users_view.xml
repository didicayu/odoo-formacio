<odoo>
    <data>
        <record id="inherited_users_form" model="ir.ui.view">
            <field name="name">estate_users.form</field>
            <field name="model">inherited_users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='preferences']" position="inside">
                    <group>
                        <field name="property_ids"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>