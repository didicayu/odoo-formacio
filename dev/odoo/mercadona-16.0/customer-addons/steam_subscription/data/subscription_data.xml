<odoo>

    <data noupdate="1">
<!--        Product Variants-->
        <record id="product_subscription_attribute" model="product.attribute">
            <field name="name">Subscription</field>
        </record>
        <!--    Specific Subscription Times-->
        <record id="subscription_attribute_value_1" model="product.attribute.value">
            <field name="name">1 Month</field>
            <field name="attribute_id" ref="product_subscription_attribute"/>
            <field name="sequence">1</field>
        </record>
        <record id="subscription_attribute_value_2" model="product.attribute.value">
            <field name="name">3 Months</field>
            <field name="attribute_id" ref="product_subscription_attribute"/>
            <field name="sequence">2</field>
        </record>
        <record id="subscription_attribute_value_3" model="product.attribute.value">
            <field name="name">6 Months</field>
            <field name="attribute_id" ref="product_subscription_attribute"/>
            <field name="sequence">3</field>
        </record>
        <record id="subscription_attribute_value_4" model="product.attribute.value">
            <field name="name">12 Months</field>
            <field name="attribute_id" ref="product_subscription_attribute"/>
            <field name="sequence">4</field>
        </record>
    </data>
</odoo>
