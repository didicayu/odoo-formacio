<odoo>
    <record id="view_print_property_form" model="ir.ui.view">
        <field name="name">print_property.wizard.form</field>
        <field name="model">print_property_wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="property_id" string="Select Property "/>
                </group>
                <footer>
                    <button name="print_property" string="Print Property" type="object" class="btn-primary" data-hotkey="q"/>
                    <button string="Discard" class="btn-secondary" special="cancel" data-hotkey="z" />
                </footer>
            </form>
        </field>
    </record>

    <record id="action_print_property" model="ir.actions.act_window">
        <field name="name">Print a property data</field>
        <field name="res_model">print_property_wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>